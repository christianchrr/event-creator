<div class="app">
  <div class="app-content">
    <div class="app-inner">
      <div class="header">
        <h1>Edit Location</h1>
      </div>
      <%= render 'form' %>
      <div class="menu">
        <%= form_with model: @location, method: "delete" do %>
          <%= submit_tag "Delete Location" %><br><br>
        <% end %>

        <%= link_to 'Back', location_path(@location) %>
      </div>
    </div>
  </div>
</div>