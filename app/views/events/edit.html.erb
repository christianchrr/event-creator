<div class="app">
  <div class="app-content">
    <div class="app-inner">
      <div class="header">
        <h1>Edit Event</h1>
      </div>
      <%= render 'form' %>
      <div class="menu">
        <%= form_with model: @event, method: "delete" do %>
          <%= submit_tag "Delete Event" %>
        <% end %><br>

        <%= link_to 'Back', event_path(@event) %>
      </div>
    </div>
  </div>
</div>